<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h2 class="ttl-text title-font text-center mrg-btm20"> Add Multiple bleapps</h2>
    </div>
  </div>
  <div class="row">
    <div class="popup-form">
    <%= form_for(:csv, :url => admin_import_csv_path,method: :post,:html => {:id => "import_csv_form",:name => "import_csv_form"}) do |f|%>
    <div class="col-md-6 col-md-push-3">
      <div class="form-group">
        <label for="Name" class="control-label">Upload CSV</label>
          <%= f.file_field :file,:id => "csv_file",:class => "form-control", :placeholder => "Enter UUID"%>
      </div>
    </div>
    <div class="clearfix"></div>
    <div class="col-md-6 col-md-push-3">
      <%= f.submit "Upload",:id => "submit_button",:class => "btn btn-popup title-font btn-lg btn-block", data: { disable_with: "Processing..." }%>
      <p>Please <a href="/sample.csv">Click Here</a> to download the sample CSV file.</p>
    </div>                
    <% end %>
   </div>
  </div>
</div>
   

